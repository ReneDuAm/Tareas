int rojo = 3;
int verde = 5;

void setup(){
pinMode(rojo, OUTPUT);
pinMode(verde, OUTPUT);
}

void loop(){
for(int i = 0; i < 10; i++){
digitalWrite(rojo, HIGH);
delay(1200);
digitalWrite(rojo, LOW);
delay(1200);
}
for(int i = 0; i < 10; i++){
digitalWrite(verde, HIGH);
delay(1200);
digitalWrite(verde, LOW);
delay(1200);
}
}
